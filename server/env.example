# Docker environment variables
POSTGRES_DB=museum_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

PGADMIN_DEFAULT_EMAIL=admin@admin.org
PGADMIN_DEFAULT_PASSWORD=admin

# GigaChat API credentials for chatbot
# Получите на https://developers.sber.ru/
# ВАЖНО: Можно указать с префиксом "Basic " или без него - код автоматически обработает
# Формат: base64 строка (например: MDE5YTU4NmUtNzk2Zi03MjZmLWFiY2MtYzI2ZTQzMDEyNjQ1OmUzZmM0ZjNjLTE0NGItNDBiNC05Y2RjLTI4NmVkOGFhMTY4OA==)
GIGACHAT_AUTH_KEY=Basic YOUR_AUTH_KEY_HERE
SCOPE=GIGACHAT_API_PERS
GIGACHAT_VERIFY_SSL_CERTS=False